<%= form_for(@list) do |f| %>
  

  <div class="form-group">
    <%= f.label :title %><br>
    <%= f.text_field :title, class: 'form-control', placeholder: 'Enter title' %>
  </div>
  <div class="form-group">
    <%= f.label :permissions %><br>
    <%= f.select :permissions, options_for_select(List.permission_options),{}, {class:'form-control'} %>
  </div>
  <div class="form-group">
    <%= f.submit "Save", class: 'btn btn-default' %>
  </div>
<% end %>
